#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chevoska/work/chevoska/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chevoska/work/chevoska/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest-cli@28.1.3_uo4il2aklsrxuk4ro37qmnu2ge/node_modules/jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../jest-cli@28.1.3_uo4il2aklsrxuk4ro37qmnu2ge/node_modules/jest-cli/bin/jest.js" "$@"
fi
