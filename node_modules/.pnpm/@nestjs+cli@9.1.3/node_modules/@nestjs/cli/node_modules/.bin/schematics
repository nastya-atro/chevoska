#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chevoska/work/chevoska/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chevoska/work/chevoska/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@angular-devkit+schematics-cli@14.2.2_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/bin/schematics.js" "$@"
else
  exec node  "$basedir/../../../../../../@angular-devkit+schematics-cli@14.2.2_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/bin/schematics.js" "$@"
fi
